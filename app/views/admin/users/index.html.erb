<div>
  <% @users.each do |user| %>
    <table>
      <tr>
        <td>
          <%= user.id %>
        </td>
        <td>
          <%= user.last_name %>
        </td>
        <td>
          <%= user.first_name %>
        </td>
        <td>
          <%= user.email %>
        </td>
        <td>
          <%= l user.created_at, format: :long %>
        </td>
        <td>
          <%= link_to "詳細", "#" %>
          <%= link_to "編集", "#" %>
          <%= link_to "削除", "#" %>
        </td>
      </tr>
    </table>
  <% end %>
</div>